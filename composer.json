{
    "name": "bitshost/upmvc",
    "description": "upMVC. Modular Model View Controller. A modular PHP MVC system with integrated routing for scalable and organized web applications. Built-in Namespace Support. Object-Oriented Programming (OOP).",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "gabordemooij/redbean": "^5.7"
    },
    "authors": [
        {
            "name": "Adrian D",
            "email": "contact@delvirai.net",
            "homepage": "https://upmvc.com",
            "role": "Developer"
        }
    ],
    "license": [
        "MIT"
    ],
    "autoload": {
        "psr-4": {
            "Admin\\": "modules/admin/",
            "Admin\\Routes\\": "modules/admin/routes/",
            "Common\\Assets\\": "common/assets/",
            "Common\\Bmvc\\": "common/bmvc/",
            "upMVC\\": "etc/",
            "upMVC\\Middleware\\": "etc/Middleware/",
            "upMVC\\Container\\": "etc/Container/",
            "upMVC\\Exceptions\\": "etc/Exceptions/",
            "upMVC\\Config\\": "etc/Config/",
            "upMVC\\Cache\\": "etc/Cache/",
            "upMVC\\Events\\": "etc/Events/",
            "Tools\\ModuleGenerator\\": "tools/modulegenerator/",
            "Tools\\ModuleGeneratorEnhanced\\": "tools/modulegenerator-enhanced/",
            "User\\": "modules/user/",
            "User\\Routes\\": "modules/user/routes/",
            "Userorm\\": "modules/userorm/",
            "Userorm\\Routes\\": "modules/userorm/routes/",
            "Test\\": "modules/test/",
            "Test\\Routes\\": "modules/test/routes/",
            "Moda\\": "modules/moda/",
            "Moda\\Routes\\": "modules/moda/routes/",
            "Suba\\": "modules/moda/modules/suba/",
            "Suba\\Routes\\": "modules/moda/modules/suba/routes/",
            "Newmod\\": "modules/newmod/",
            "Newmod\\Routes\\": "modules/newmod/routes/",
            "Auth\\": "modules/auth/",
            "Auth\\Routes\\": "modules/auth/routes/",
            "Mail\\": "modules/mail/",
            "PHPMailer\\PHPMailer\\": "modules/mail/phpmailer/",
            "React\\": "modules/react/",
            "React\\Routes\\": "modules/react/routes/",
            "React\\Component\\": "modules/react/etc/",
            "Reactb\\": "modules/reactb/",
            "Reactb\\Routes\\": "modules/reactb/routes/",
            "Reactcrud\\": "modules/reactcrud/",
            "Reactcrud\\Routes\\": "modules/reactcrud/routes/",
            "Dashboard\\": "modules/dashboard/",
            "Dashboard\\Routes\\": "modules/dashboard/routes/",
            "Dashboardexample\\": "modules/dashboardexample/",
            "Dashboardexample\\Routes\\": "modules/dashboardexample/routes/",
            "Enhanced\\": "modules/enhanced/",
            "Enhanced\\Routes\\": "modules/enhanced/routes/",
            "Testitems\\": "modules/testitems/",
            "Testitems\\Routes\\": "modules/testitems/routes/",
            "Reactnb\\": "modules/reactnb/",
            "Reactnb\\Routes\\": "modules/reactnb/routes/",
            "Reacthmr\\": "modules/reacthmr/",
            "Reacthmr\\Routes\\": "modules/reacthmr/routes/"
        }
    }
}